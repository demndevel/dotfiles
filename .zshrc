POWERLEVEL9K_NODE_ICON='%fvia %F{green}⬢'
 
############ END- POWERLEVEL THEME SETTINGS ##############

# To customize prompt, run `p10k configure` or edit ~/.p10k.zsh.
[[ ! -f ~/.p10k.zsh ]] || source ~/.p10k.zsh


# history of commands
HISTFILE=~/.histfile
HISTSIZE=1000
SAVEHIST=1000

# Golang da
export GOPATH=$HOME/go 
export PATH=$PATH:$GOPATH/bin
